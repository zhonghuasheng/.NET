<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework, Version=v4.5"/>
  </startup>

  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="metadataBehavior">
          <serviceMetadata httpGetEnabled="true" httpGetUrl="http://127.0.0.1/caculatorsercice/metadata"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>

    <services>
      <service behaviorConfiguration="metadataBehavior" name="Service.CalculatorService">
        <endpoint address="http:127.0.0.1:3721/caculatorservice" binding="wsHttpBinding" contract="Service.Interface.ICalculator" />
      </service>
    </services>
  </system.serviceModel>
</configuration>